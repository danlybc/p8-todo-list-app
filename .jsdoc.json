{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "exclude": ["assets/js/lib", "assets/static", "build", "public", "node_modules", "debug", "docs", "error"],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "theme_opts": {
      "title": "todo-list-app",
      "menu": [{
        "title": "Ouvrir le repo GitHub ðŸ”—",
        "link": "https://github.com/danlybc/p9-todo-list-app",
        "target": "_blank"
      }, {
        "title": "Ouvrir l'audit (format pdf) ðŸ”—",
        "link": "audit.pdf",
        "target": "_blank"
      }],
      "add_style_path": ["custom.css"]
    },
    "recurse": true,
    "readme": "./README.md",
    "destination": "./docs/",
    "template": "node_modules/jsdoc-template"
  },
  "jsVersion": 180
}